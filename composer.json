{
    "name": "chweb/multi-auth",
    "description": "Laravel 12 Multi-Auth Package for PHP 8.4",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Harshit Chavda",
            "email": "chwebtech0811@gmail.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/console": "^11.0|^12.0",
        "symfony/process": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Chweb\\MultiAuth\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chweb\\MultiAuth\\MultiAuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "homepage": "https://github.com/harshitchavda11/multi-auth",
    "keywords": [
        "laravel",
        "multi-auth",
        "authentication"
    ],
    "support": {
        "issues": "https://github.com/harshitchavda11/multi-auth/issues",
        "source": "https://github.com/harshitchavda11/multi-auth"
    }
}